{
  "name": "YouTube Repeat",
  "version": "1.0.2",
  "description": "Repeat YouTube videos.",
  "icons": {
    "16": "icons/16.png",
    "22": "icons/22.png",
    "32": "icons/32.png",
    "48": "icons/48.png"
  },
  "manifest_version": 2,
  "content_scripts": [{
    "matches": ["http://*.youtube.com/*", "https://*.youtube.com/*"],
    "js": ["contentscript.js"]
  }]
}
